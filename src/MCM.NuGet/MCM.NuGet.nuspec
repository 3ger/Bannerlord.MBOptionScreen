<?xml version="1.0" encoding="utf-8"?>

<package>
  <metadata>
    <id>Bannerlord.MCM</id>
    <version>$version$</version>
    <title>Mod Configuration Menu</title>
    <owners>Aragas</owners>
    <authors>Aragas, mipen</authors>
    <description>A library for easy Setting integration. Contains the abstraction library, requires the Standalone dependency to work!</description>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <icon>icon.png</icon>
    <iconUrl>https://github.com/Aragas/Bannerlord.MBOptionScreen/blob/master/MCM-128-w-2.png?raw=true</iconUrl>
    <license type="expression">MIT</license>
    <repository type="git" url="https://github.com/Aragas/Bannerlord.MBOptionScreen.git" />
    <tags></tags>
  </metadata>
  <dependencies>
    <group targetFramework=".NETFramework4.7.2">
      <dependency id="Bannerlord.ButterLib" version="0.0.6" exclude="Runtime,Build,Native,Analyzers,BuildTransitive" />
      <dependency id="Lib.Harmony" version="2.0.2" exclude="Runtime,Build,Native,Analyzers,BuildTransitive" />
    </group>
  </dependencies>
  <files>
    <file src="bin/$configuration$/$targetframework$/MCM-128-w-2.png" target="icon.png" />
    <file src="bin/$configuration$/$targetframework$/MCMv3.dll" target="lib/$targetframework$" />
    <file src="bin/$configuration$/$targetframework$/MCMv3.pdb" target="lib/$targetframework$" />
  </files>
</package>
